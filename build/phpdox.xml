<?xml version="1.0" encoding="UTF-8"?>
<phpdox xmlns="http://phpdox.net/config">
    <project name="Curl" source="../src" workdir="phpdox">
        <collector publiconly="false">
            <include mask="*.php" />
        </collector>

        <generator output="./">
            <build engine="html" enabled="true" output="api">
                <file extension="html" />
            </build>
        </generator>
    </project>
</phpdox>
